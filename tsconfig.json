{
	"compilerOptions": {
		"allowJs": true,
		"allowSyntheticDefaultImports": true,
		"baseUrl": ".",
		"esModuleInterop": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"lib": [
			"DOM",
			"ESNext"
		],
		"module": "ESNext",
		"moduleResolution": "node",
		"noEmit": true,
		"paths": {
			"src/*": [
				"src/*"
			]
		},
		"strict": true,
		"target": "ESNext",
	}
}
