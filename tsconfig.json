{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"baseUrl": ".",
		"esModuleInterop": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"jsxImportSource": "solid-js",
		"module": "ESNext",
		"moduleResolution": "node",
		"noEmit": true,
		"paths": {
			"src/*": [
				"src/*"
			]
		},
		"target": "ESNext",
		"types": [
			"vite/client"
		],
	}
}
